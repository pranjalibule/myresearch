        LoadTSRStatus();
        if (roleId == "Guest" || roleId == "guest") {
            $(".GuestClass").hide();
            $(".GuestInfo").show();
        }
        else if (roleId.toLowerCase() == "account manager") {
            $("#trNewTSR").hide();
            $(".GuestInfo").show();
        }
        else {
            $(".GuestClass").show();
            $(".GuestInfo").hide();
            $("#trNewTSR").show();
