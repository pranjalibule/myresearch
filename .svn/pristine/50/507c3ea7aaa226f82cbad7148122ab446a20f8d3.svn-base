body {
    float: left;
    width: 99%;
    overflow:hidden;
    background-color:#d7e3e3;
    /*padding:0;
    margin:0;*/
}

div {
    float: left;
}

.leftDiv {
    width: 12%;
    height: 890px;
    margin-right: 5px;
}


 a {
     text-decoration:none;
    font-size:15px;
    color:#0f28bb;    
}

.rightDiv {
    border-left: 1px solid black;
    width: 85%;
    height: 90vh;
    padding: 0px 10px;
    /*min-height: 920px;*/
    padding-top:5px;
}

.heading {
    float: left;
    width: 100%;
}

    .heading tr {
        float: left;
        /*padding: 5px 0px;*/
        border-radius: 3px;
        width: 100%;
    }

    .heading td {
        float: left;
        /*padding: 5px 0px;*/
        border-radius: 3px;
        width: 100%;
        text-align: center;
    }

.headline {
    float: left;
    color: black;
    font-size: 20px;
    background-color: #8aab7f;
    font-weight: bolder;
    width: 100%;
    align-content: center;
}

    .headline td {
        float: left;
        padding: 5px;
        border-radius: 3px;
    }

input[type="button"], input[type="submit"], button {
    border: .5px solid #0d6d69;
    float: left;
    color: white;
    font-weight: bolder;
    padding: 5px 10px;
    border-radius: 3px;
    background-color: #0d6d69;
    cursor: pointer;
    margin: 5px;
}

.table {
    float: left;
    padding: 5px;
    width: 100%;
}

.table {
    float: left;
    padding: 5px;
    border-collapse: separate;
    border-spacing: 2px;
}

    .table th {
        /*float:left;*/
        padding: 5px;
        border-radius: 0px;
        margin-right: 3px;
        background-color: #b5bd3a;
        text-align: center;
    }

th:first-child {
    border-radius: 5px 0px 0px 0px;
}

th:last-child {
    border-radius: 0px 5px 0px 0px;
}

tr:last-child td:first-child {
    border-radius: 0px 0px 0px 5px;
}

tr:last-child td:last-child {
    border-radius: 0px 0px 5px 0px;
}

.table tr:nth-child(odd) {
    background-color: #d6d8e5;
}

.table tr:nth-child(even) {
    background-color: #e6e6c1;
}

.table td {
    /*float:left;*/
    padding: 5px;
    text-align: center;
    vertical-align: top;
}

.table .description {
    text-align: justify;
}

.table .leftalign {
    text-align: left;
}

#lblUser {
    font-size: 26px;
    width:100%;
}

#logOut {
    border: .5px solid #9e2222;
    text-decoration: none;
    color: white;
    font-weight: bolder;
    padding: 5px 10px;
    border-radius: 3px;
    background-color: #9e2222;
    cursor: pointer;
    margin: 5px;
}

.tsoPage {
    float: left;
    width: 100%;
}

    .tsoPage tr {
        float: left;
        width: 100%;
        min-height:40px;
    }

    .tsoPage td {
        float: left;
        width: 97.5%;
    }

.pageHeading {
    float: left;
    width: 100%;
    background-color: #8aab7f;
