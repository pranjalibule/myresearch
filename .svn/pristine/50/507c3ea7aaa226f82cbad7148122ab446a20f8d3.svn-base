    width: 54% !important;
}

.ui-datepicker {
    background-color: #e5dfd7;
    position: absolute;
    z-index: 1;
    padding: 3px;
    border-radius: 3px;
    float: left;
    width: 10.5em;
    display: none;
}

    .ui-datepicker div {
        float: left;
    }

.ui-datepicker-header {
    width: 150px;
}

select {
    border: 1px solid grey;
    padding: 3px 2px;
    border-radius: 3px;
}

.ui-datepicker select {
    margin: 5px;
    float: left;
}

.ui-datepicker-prev, .ui-datepicker-next {
    border: .5px solid #fccd76;
    float: left;
    color: black;
    font-weight: bolder;
    padding: 5px 10px;
    border-radius: 3px;
    background-color: #fcd180;
    cursor: pointer;
    margin: 5px;
}

textarea {
    float: left;
    padding: 3px 2px;
    border-radius: 3px;
    margin-top: 3px;
    border: 1px solid grey;
}

.info {
    background-color: #f8f2bb;
    margin-bottom:15px;
}

.smallTiletd {
    width: 18% !important;
}

.selectionSelectBox {
    float: left;
    width: 100%;
}

    .selectionSelectBox td {
        float: left;
        width: 43.5%;
    }

#tblMain {
    float: left;
    text-align: left;
    padding: 0px 10px;
    border-collapse: separate;
    border-spacing: 2px;
}

.leftMenu {
    float: left;
    text-align: center;
    margin: 5px;
}

    .leftMenu a {
        float: left;
        text-decoration: none;
        background-color: #804a5d;
        border-radius: 3px;
        padding: 25px;
        width: 350px;
        color: white;
    }

        .leftMenu a:hover {
            background-color: #e8eece;
            color: black;
        }

.leftMenuSmall a {
    float: left;
    text-decoration: none;
    background-color: #804a5d;
    border-radius: 3px;
    padding: 8px;
    width: 152px;
    color: white;
}

    .leftMenuSmall a:hover {
        background-color: #e8eece;
        color: black;
    }

.leftMenuSmallselect a {
    float: left;
    text-decoration: none;
    border-radius: 3px;
    padding: 8px;
    width: 152px;
    background-color: #f7e04a;
    cursor: not-allowed;
}

.ui-autocomplete {
    background-color: #d8bf91;
    width: 150px;
    list-style: none;
    padding: 2px;
    border-radius: 3px;
}

    .ui-autocomplete li {
        float: left;
        width: 95%;
        padding: 3px;
        margin: 2px;
    }

.loader {
    border: 16px solid #f3f3f3;
    border-radius: 50%;
    border-top: 16px solid #3498db;
    width: 120px;
    height: 120px;
    -webkit-animation: spin 2s linear infinite;
    animation: spin 2s linear infinite;
}

@-webkit-keyframes spin {
    0% {
        -webkit-transform: rotate(0deg);
    }

    100% {
        -webkit-transform: rotate(360deg);
    }
}

@keyframes spin {
    0% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(360deg);
    }
}

