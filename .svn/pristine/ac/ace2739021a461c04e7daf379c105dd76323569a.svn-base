                            sb.Append(0).Append(",");                           //"Defect Density"
                            sb.Append(0).Append(",");                    //"Defect Rejection Ratio"

                            sb.Append(0).Append(",");                                                                 //Headcount
                            sb.Append(string.Format("{0:0.00}", 0)).Append(",");                                                                 //IdleTimeDuration
                            sb.Append(string.Format("{0:0.00}", 0)).Append(",");                                                                 //IdleTimeDuration 
                            sb.Append(sdcTask.PlannedStartDate.ToString("dd-MMM-yyyy")).Append(",");                                            //Start date
                            sb.Append(sdcTask.PlannedCompletionDate.ToString("dd-MMM-yyyy")).Append(",");                                 //Target Completion Date
                            sb.Append(string.IsNullOrEmpty(sdcTask.TSOServiceDeliveryChain.TSO.TSR.AccountManager.Name) ? string.Empty : string.Format("\"{0}\"", sdcTask.TSOServiceDeliveryChain.TSO.TSR.AccountManager.Name)).Append(","); //Account Manager	
                            sb.Append(string.IsNullOrEmpty(sdcTask.TSOServiceDeliveryChain.TSO.TSR.DeliveryManager.Name) ? string.Empty : string.Format("\"{0}\"", sdcTask.TSOServiceDeliveryChain.TSO.TSR.DeliveryManager.Name)).Append(",");// Delivery manager
                            sb.Append(string.IsNullOrEmpty(sdcTask.TSOServiceDeliveryChain.TSO.TSR.TestManager.Name) ? string.Empty : string.Format("\"{0}\"", sdcTask.TSOServiceDeliveryChain.TSO.TSR.TestManager.Name)).Append(",");       // Test Manager	
