<!doctype html>
<html>
<head>
	<meta charset="utf-8">
	<title>TTM Portal login</title>
	<script src="/scripts/dateformt.js"></script>
	<script src="/scripts/jquery-3.1.1.min.js"></script>
	<script src="/scripts/common.js"></script>
    <script src="/scripts/jquery-ui.js"></script>
    <link rel="stylesheet" href="/styles/jquery-ui.css" />
	<link rel="stylesheet" href="/Styles/css.css" />
</head>
<body>
	<form action="#">
		<div class="credential">
			<div style="margin-bottom:25px;width: 100%;"><input type="text" id="username" onkeyup="DoLogin(event)" placeholder="User Name"></div>
			<div style="margin-bottom:25px;width: 100%;"><input type="password" id="password" onkeyup="DoLogin(event)"  placeholder="Password"></div>
			<div><label id="txtOutput"></label></div>
			<div class="button" style="width: 100%;text-align:center;"><button type="button" onclick="Login()" class="btn_login">Login</button></div><br />
			<div class="username" style="width: 100%;text-align:center; margin-top:15px;"><a href="#" onclick="RequestAccess()">Request for Access</a></div>
		</div>
		
	</form>
	<div id="footer"><div class="footer_text">SQS India© 2017 copyright  |  <a href="#">Disclaimer</a></div></div>
	<script type="text/javascript" src="/scripts/index.js"></script>
    <script>
        $(document).ready(function () {
            $('#username').val('');
            $('#password').val('');
        });
    </script>
</body>
</html>