.leftDiv {
    height: 0% !important;
}

.rightDiv {
    height: 88vh !important;
    overflow-y: auto;
    overflow-x: hidden;
    padding-top: 5px;
    width: 98.5%;
}

.scrollDiv {
    height: auto !important;
    float: left;
    width: 100%;
    overflow: hidden;
}

.topMenuRow {
    margin-top: 5px;
    height: 25px;
    width: 100%;
}

.bottomMenuRow {
    height: 10px;
    width: 100%;
    margin-bottom: 1%;
    background-color: #d4d984;
}


.masterMenu {
    margin: 0 5px;
    padding: 8px 13px;
    cursor: pointer;
    background-color: #165f7c;
    color: white;
}

.bottombottomMenuRowSelected {
    background-color: #fb8c20;
}

.scrollDiv {
    float: left;
    width: 100%;
    height: 66% !important;
    overflow-y: auto;
    overflow-x: hidden;
    border-top: 1px ridge black;
}

.rightDiv {
    height: 89vh !important;
}

.smallTiletd {
    width: 8% !important;
}

fieldset {
    width: 98.5% !important;
    margin-bottom: 10px;
}

@media only screen and (min-device-width: 768px) and (max-device-width: 1024px) and (-webkit-min-device-pixel-ratio: 1) {
    .rightDiv {
        height: 55vh !important;
    }

    .scrollDiv {
        height: 66% !important;
    }
}
