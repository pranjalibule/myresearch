        function SortByDisplayOrder(a, b) {
            var firstDisplay = parseInt(a.DisplayOrder);
            var secondDisplay = parseInt(b.DisplayOrder);
            return ((firstDisplay < secondDisplay) ? -1 : ((firstDisplay > secondDisplay) ? 1 : 0));
        }


