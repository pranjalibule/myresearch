            TSOServiceDeliveryChainTasks = new HashSet<TSOServiceDeliveryChainTaskDTO>();
        }
        [JsonProperty("ID")]
        public int ID { get; set; }

        [JsonProperty("TSOId")]
        public int TSOId { get; set; }

        [JsonProperty("ServiceDeliveryChainId")]
